    "#Ta koda standardizira datume in pripravi da potem lažje ločim uro od datuma.\n",
    "zlato['Datum'] = pd.to_datetime(zlato['Datum'], infer_datetime_format=True)\n",
    "zlato['Datum'] = zlato['Datum'].apply(lambda x:'{}.{}.{}//{}.0{}'.format(x.day,x.month,x.year,x.hour,x.minute))# axis=1)\n",
    "zlato"
   ]
