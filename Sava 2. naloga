   "source": [
    "#Ta funkcija sprejme float in vrne string v katerem je zapisan float na način, da so tisočice ločene z piko, števila pa imajo samo eno decimalno mesto.\n",
    "def lepe_s(input):\n",
    "    input = round(input,1)\n",
    "    sep = '.' if locale.localeconv()['thousands_sep'] == '.' else '.' #get locale sep\n",
    "    parts = str(input).split(sep) #get part before and after the sep\n",
    "    parts[0] = locale.format_string(\"%d\", float(parts[0]), grouping=True)#format **part 0**\n",
    "    parts[0] = parts[0].replace(',','.')\n",
    "    return ','.join(parts) #rejoin\n",
    "\n",
    "\n"
   ]
  },
